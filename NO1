#include <iostream>

using namespace std;

int main () {
    int x,y,z;
    int a,b;
    x=0;
    y=1;

    cout << "Masukan angka :";
    cin >> b;

    if (b<2) {
        cout <<"Banyaknya deret lebih dari 1.";
        goto selesai;
    }

    cout <<x<<", "<<y<<", ";

    for (a=2; a<b; a++) {
        z=x+y ;

    cout <<z<<", ";
    x=y;
    y=z;

    }

selesai :

return 0;
}
